	\title{Harvesting Effects in Reaction-Diffusion-Advection Model: Exploring Decoupled Algorithms and Analysis}
	
	\author[1]{\small Mayesha Sharmim Tisha\thanks{Email: mayeshasharmim-2015617107@math.du.ac.bd}}
	\author[1]{\small Md Kamrujjaman\thanks{Corresponding author: Md Kamrujjaman, E-mail: kamrujjaman@du.ac.bd}}
	\author[2]{\small  Muhammad Mohebujjaman\thanks{Email: mmohebuj@uab.edu}}%, \\ ORCID: \url{https://orcid.org/0000-0003-4278-7078}}} 	
	\author[3]{\small Taufiquar Khan\thanks{Email: taufiquar.khan@uncc.edu} }
