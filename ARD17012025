\begin{comment}
    \begin{appendices}

		%\newpage
		\section{Appendix} \label{appendix} \noindent Denote $D_T=(0,T]\times \Omega$, $\partial D_T=(0,T]\times \partial \Omega$. Consider the nonlinear parabolic equation
\begin{align} 
	\label{parabolic}
	\begin{cases}
		\vspace{0.2cm}
		&\frac{\partial u}{\partial t}-Lu=f(t,\bx,u),\hspace{2mm}\forall (t,{\bx})\in D_T,\\
		\vspace{0.2cm}
		&d_1\frac{\partial u}{\partial \bn}-\beta_1 u\frac{\partial K}{\partial \bn}=0,\ \displaystyle \bx\in\partial \Omega,\\
		\vspace{0.2cm}
		&	u(\bx,0)={{u}^0}(\bx)\geq,\not\equiv 0,\ \displaystyle \bx\in \Omega.
	\end{cases}
\end{align}
where the operator $L$\ is defined as
$$L u:= \sum\limits_{i,j=1}^da_{ij}\frac{\partial^2u}{\partial x_i\partial x_j}+\sum_{i,j=1}^{d}b_i(t,\bx)\frac{\partial u}{\partial x_j}$$
is uniformly elliptic, namely there exist positive numbers $\lambda$\ and $\Lambda$\ such that for every non-zero vector $\bxi=(\xi_1,\cdots,\xi_d)\in \mathbb{R}^d$
$$\lambda|\bxi|^2\le \sum\limits_{i,j=1}^da_{ij}(t,\bx)\xi_i\xi_j\le \Lambda |\bxi|^2, (t,\bx)\in  \overline{D_T}$$
where the coefficients of $L$\ are  H\"{o}lder continuous in $D_T.$\\

\textbf{Super-solution:} A function $\overline{u}(t,\bx)\in C(\overline{D_T})\cap C^{1,2}(D_T)$\ is called an upper (super) solution of \eqref{parabolic} if it satisfies the following:
 \begin{align} \label{super-solution}
 	\begin{cases}
 		&\frac{\partial \overline{u}}{\partial t}-L\overline{u}\ge f(t,\bx,\overline{u}),\hspace{2mm}\forall (t,{\bx})\in D_T,\\
 		\vspace{0.2cm}
 		&d_1\frac{\partial \overline{u}}{\partial \bn}\ge \beta_1 \overline{u}\frac{\partial K}{\partial \bn},\ (t,{\bx})\in \partial D_T,\\
 		\vspace{0.2cm}
 		&	\overline{u}(\bx,0)\ge{{u}^0}(\bx)\geq,\not\equiv0,\ \displaystyle \bx\in \Omega.
 		\end{cases}
 \end{align}

\textbf{Sub-solution}: A function $\underline{u}(t,x)\in C(\overline{D_T})\cap C^{1,2}(D_T)$\ is called an lower(sub) solution of \eqref{parabolic} if it satisfies the following inequalities
\begin{align} \label{sub-solution}
	\begin{cases}
	\vspace{0.2cm}
	&\frac{\partial \underline{u}}{\partial t}-L\underline{u}\le f(t,\bx,\underline{u}),\hspace{2mm}\forall (t,{\bx})\in D_T,\\
	\vspace{0.2cm}
	&d_1\frac{\partial \underline{u}}{\partial \bn}\le \beta_1 \underline{u}\frac{\partial K}{\partial \bn},\ \displaystyle (t,{\bx})\in \partial D_T,\\
	\vspace{0.2cm}
	&	\textcolor{red}{\underline{u}(\bx,0)\le{{u}^0}(\bx),\; \bx\in \Omega.}
	\end{cases}
\end{align}
The pair $(\overline{u},\underline{u})$\ is said to be ordered if $\overline{u}\ge \underline{u}$\ in $D_T$.
  \end{appendices}
\end{comment}
