\begin{document}
\title{Stocking and Harvesting Effects in Advection-Reaction-Diffusion Model: Exploring Decoupled Algorithms and Analysis}



\author{
Mayesha Sharmim Tisha\footremember{DU}{D\MakeLowercase{epartment of} M\MakeLowercase{athematics}, U\MakeLowercase{niversity of} D\MakeLowercase{haka}, D\MakeLowercase{haka} 1000, B\MakeLowercase{angladesh}.}\hspace{-1ex}\hspace{1ex}\footnote{D\MakeLowercase{epartment of} M\MakeLowercase{athematics}, A\MakeLowercase{merican} I\MakeLowercase{nternational} U\MakeLowercase{niversity-}B\MakeLowercase{angladesh},  D\MakeLowercase{haka} 1229, B\MakeLowercase{angladesh}.}
