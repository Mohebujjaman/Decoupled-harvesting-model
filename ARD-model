	\title{Harvesting Effects in Reaction-Diffusion-Advection Model: Exploring Decoupled Algorithms and Analysis}
	
	\author[1]{\small Mayesha Sharmim Tisha\thanks{Email: mayeshasharmim-2015617107@math.du.ac.bd}}
