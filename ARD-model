\begin{comment}
	\newpage
	\section*{Appendix} \label{appendix} Here we
	find the restriction on the time-step size to have the stability of the Algorithm \ref{Algn1}. We consider the following linear system:
	\begin{align}
		a(u_{i,h}^{n+1},v_{h})=F\left(v_{h}\right),\hspace{1mm}\forall v_{h}\in X_h,\hspace{1mm}i=1,2,\cdots,N,\label{linear-system}
	\end{align}
	where the linear form
	\begin{align}
		F\left(v_{h}\right):=\frac{1}{\Delta t}\left(u_{i,h}^{n},v_{h}\right)+\left(f_i(t^{n+1}),v_{h}\right),
	\end{align}
	and  the  bilinear form
	
	\begin{align}
		a\left(u_{i,h}^{n+1},v_{h}\right):&=\frac{1}{\Delta t}\left(u_{i,h}^{n+1},v_{h}\right)+\mathcal{D}_i\left(\nabla u_{i,h}^{n+1},\nabla v_{h}\right)-\beta_i\left( u_{i,h}^{n+1}\nabla K(t^{n+1}),\nabla v_{h}\right)-\nonumber\\
		&(1-\gamma_i)\left(r_i(t^{n+1})u_{i,h}^{n+1},v_{h}\right)-\left(\frac{r_i(t^{n+1})u_{i,h}^{n+1}}{K(t^{n+1})}\sum\limits_{j=1}^Nu_{j,h}^n,v_{h}
		\right).\label{bilinear-form}
	\end{align}
	Now, substitute $v_{h}=u_{i,h}^{n+1}$ in \eqref{bilinear-form} to give
	\begin{align}
		a&\left(u_{i,h}^{n+1},u_{i,h}^{n+1}\right)=\frac{1}{\Delta t}\|u_{i,h}^{n+1}\|^2+\mathcal{D}_i\|\nabla u_{i,h}^{n+1}\|^2-\beta_i\left( u_{i,h}^{n+1}\nabla K(t^{n+1}),\nabla v_{h}\right)-(1-\gamma_i)\left(r_i(t^{n+1})u_{i,h}^{n+1},u_{i,h}^{n+1}\right)\nonumber\\
		&-\left(\frac{r_i(t^{n+1})u_{i,h}^{n+1}}{K(t^{n+1})}\sum\limits_{j=1}^Nu_{j,h}^n,u_{i,h}^{n+1}
		\right)\nonumber\\
		&\ge \frac{1}{\Delta t}\|u_{i,h}^{n+1}\|^2+\mathcal{D}_i\|\nabla u_{i,h}^{n+1}\|^2-C\|\nabla u_{i,h}^{n+1}\|^2\|K(t^{n+1})\|_{H^2(\Omega)^d}-|1-\gamma_i|\|r_i(t^{n+1})\|_{\infty}\|u_{i,h}^{n+1}\|^2-\nonumber\\
		&~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\frac{C\|r_i(t^{n+1})\|_{\infty}}{\inf\limits_\Omega\|K(t^{n+1})\|}\|u_{i,h}^{n+1}\|^2.
	\end{align}
	%For the first term on the right-hand-side of \eqref{pol-new}, we use H\"{o}lder's inequality, Sobolev embedding theorem, and Poincar\'e inequalities, to find the following estimate
	%\begin{align*}
	%	\left(u_{i,h}^{n+1}\nabla K(t^{n+1}),\nabla u_{i,h}^{n+1}\right)&\le\|u_{i,h}^{n+1}\|_{L^3}\|\nabla K(t^{n+1})\|_{L^6}\|\nabla u_{i,h}^{n+1}\|\\&\le C\|u_{i,h}^{n+1}\|^{\frac12}\|\nabla u_{i,h}^{n+1}\|^{\frac12} \| K(t^{n+1})\|_{H^2(\Omega)}\|\nabla u_{i,h}^{n+1}\|\\&\le C\|\nabla u_{i,h}^{n+1}\|^2\|K(t^{n+1})\|_{H^2(\Omega)^d}.
	%\end{align*}
	The last term in the above inequality is derived as the lower bound subject to the Lemma \ref{assumption-1}. Rearranging
	\begin{align}
		&a\left(u_{i,h}^{n+1},u_{i,h}^{n+1}\right)\ge\left(\frac{1}{\Delta t}-|1-\gamma_i|\|r_i\|_{L^\infty\big(0,T;L^\infty(\Omega)^d\big)}-\frac{C\|r_i\|_{L^\infty\big(0,T;L^\infty(\Omega)^d\big)}}{\inf\limits_{(t,{\bf x})\in (0,T]\times\Omega}|K|}\right)\|u_{i,h}^{n+1}\|^2+\nonumber\\
		&~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\mathcal{D}_i\|\nabla u_{i,h}^{n+1}\|^2-C\|\nabla u_{i,h}^{n+1}\|^2\|K(t^{n+1})\|_{H^2(\Omega)^d}.
	\end{align}
	To have the coercivity condition, we must have
	\begin{align*}
		\frac{1}{\Delta t}-|1-\gamma_i|\|r_i\|_{\infty,\infty}-\frac{C\|r_i\|_{\infty,\infty}}{K_{\min}}\ge 0,
	\end{align*}
	which gives the following restriction on the time-step size
	\begin{align}
		\Delta t\le\frac{K_{\min}}{|1-\gamma_i|\|r_i\|_{\infty,\infty}K_{\min}+C\|r_i\|_{\infty,\infty}}.\label{time-step-restriction}
	\end{align}
	In a similar approach, we can find the time-step restriction on the stability of the Algorithm \ref{Algn2}.
\end{comment}
